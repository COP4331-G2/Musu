<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
<shape  android:shape="rectangle">
    <stroke android:width="1dp" android:color="@color/iron" />
    <solid android:color="@color/white" />
    <corners android:radius="15dp" />
    <padding
        android:left="15dp"
        android:top="10dp"
        android:bottom="10dp"
        android:right="55dp" />

</shape>
    </item>

  <item android:left="52dp" android:right="-52dp" android:top="15dp" android:bottom="15dp">
      <rotate
          android:fromDegrees="45"
          android:toDegrees="45">
          <shape android:shape="line">
              <stroke android:width="2dp" android:color="@color/jumbo"/>

          </shape>
      </rotate>

  </item>

    <item android:left="52dp" android:right="-52dp" android:top="15dp" android:bottom="15dp">
        <rotate
            android:fromDegrees="135"
            android:toDegrees="135">
            <shape android:shape="line">
                <stroke android:width="2dp" android:color="@color/jumbo"/>

            </shape>
        </rotate>

    </item>

    <item android:left="30dp" android:top="1dp" android:bottom="1dp" android:right="-30dp">
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90">
        <shape android:shape="line">
            <stroke android:width="2dp" android:color="@color/base"/>
        </shape>
        </rotate>
    </item>

</layer-list>


